<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="MocoappConnection">
        <property name="mocoappHost" type="string"/>
        <property name="mocoappToken" type="string"/>
    </transfer>

    <transfer name="MocoappTimeEntryCollection">
        <property name="mocoappTimeEntries" type="MocoappTimeEntry[]"/>
        <property name="lastImport" type="string"/>
    </transfer>

    <transfer name="MocoappTimeEntry">
        <property name="componentIdentifier" type="string"/>
        <property name="date" type="string"/>
        <property name="description" type="string"/>
        <property name="projectId" type="string"/>
        <property name="taskId" type="string"/>
        <property name="hours" type="string"/>
    </transfer>
</transfers>
